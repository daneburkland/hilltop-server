generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model Test {
  id       Int     @default(autoincrement()) @id
  title    String
  author   User?   @relation(fields: [authorId], references: [id])
  authorId String?
  code     String?
}

model User {
  id    String  @id
  email String  @unique
  name  String?
  tests Test[]
}
