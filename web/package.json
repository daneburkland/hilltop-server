{
  "name": "hilltop-web",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn generate && tsc",
    "generate:prisma": "prisma generate",
    "generate:nexus": "NODE_ENV=development ts-node --transpile-only src/schema --nexus-exit",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "postinstall": "yarn generate",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "migrate-save": "prisma migrate save --experimental",
    "migrate-up": "prisma migrate up --experimental",
    "studio": "prisma studio --experimental"
  },
  "dependencies": {
    "@nexus/schema": "^0.14.0",
    "@types/dockerode": "^2.5.34",
    "@types/pg": "^7.14.4",
    "@types/puppeteer": "^3.0.1",
    "@types/uuid": "^8.0.0",
    "@xstate/test": "^0.4.0",
    "aws-sdk": "^2.716.0",
    "bcryptjs": "2.4.3",
    "bull": "^3.15.0",
    "del": "^5.1.0",
    "dockerode": "^3.2.0",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "graphql-shield": "5.7.3",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.1",
    "nexus-plugin-prisma": "^0.17.0",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.10",
    "pg": "^8.3.0",
    "pino": "^6.3.2",
    "split2": "^3.1.1",
    "vm2": "^3.9.2",
    "xstate": "^4.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/bull": "^3.14.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "12.12.47",
    "@types/ws": "7.2.5",
    "ts-node": "8.10.2",
    "ts-node-dev": "1.0.0-pre.49",
    "typescript": "3.9.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
